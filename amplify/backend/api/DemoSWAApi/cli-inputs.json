{
  "version": 1,
  "paths": {
    "/product/items": {
      "name": "/product/items",
      "lambdaFunction": "DemoProductLambda",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/cart/items": {
      "name": "/cart/items",
      "lambdaFunction": "DemoCartLambda",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    }
  }
}